{
  "name": "vue-store",
  "version": "1.0.0",
  "description": "simple vue store plugin",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --hot --config ./build/webpack.dev.config.js",
    "build": "tsc -p tsconfig.prod.json & webpack --config ./build/webpack.prod.config.js",
    "build:ts": "tsc -p tsconfig.prod.json",
    "prettier": "prettier --write --config ./.prettierrc './src/**/*.{ts,tsx}'",
    "lint": "eslint '**/*.vue' --ignore-pattern node_modules/",
    "lint:fix": "eslint '**/*.vue' --fix --ignore-pattern node_modules/",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^8.4.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "vue": "^3.2.31",
    "vue-loader": "^17.0.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  }
}

{
  "name": "@v/store",
  "version": "0.1.0",
  "description": "Minimalistic and lightweight Vue 3 composition API-compatible store pattern plugin",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "webpack serve --hot --config ./build/webpack.dev.config.js",
    "build": "tsc -p tsconfig.json & webpack --config ./build/webpack.prod.config.js",
    "prettier": "prettier --write --config ./.prettierrc './src'",
    "lint": "eslint '**/*.ts' --ignore-pattern node_modules/",
    "lint:fix": "eslint '**/*.ts' --fix --ignore-pattern node_modules/",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiseadme/vue-store.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/wiseadme/vue-store/issues",
    "email": "wiseadme@gmail.com"
  },
  "keywords": [
    "vue",
    "vue-store",
    "store",
    "composition api"
  ],
  "author": {
    "name": "Anar Allkhverdiev",
    "email": "wiseadme@gmail.com",
    "url": "https://projs.net"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^8.4.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "vue": "^3.2.31",
    "vue-loader": "^17.0.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  }
}
